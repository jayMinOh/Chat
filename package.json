{
  "name": "chat",
  "version": "1.0.0",
  "description": "webChat",
  "main": "index.js",
  "author": "osm077",
  "license": "MIT",
  "devDependencies": {
    "asar": "^3.0.3",
    "electron": "^8.2.1",
    "electron-builder": "^22.4.1",
    "electron-packager": "^14.2.1"
  },
  "build": {
    "productName": "chat",
    "appId": "com.electron.hello",
    "asar": true,
    "protocols": {
      "name": "chat",
      "schemes": [
        "chat"
      ]
    },
    "mac": {
      "target": [
        "default"
      ],
      "icon": "./resources/installer/Icon.icns"
    },
    "dmg": {
      "title": "chat",
      "icon": "./resources/installer/Icon.icns"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./resources/installer/Icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "zip",
        "tar.gz"
      ],
      "icon": "./resources/linuxicon"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "resources/installer/",
      "output": "dist/",
      "app": "."
    }
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-redis": "^5.0.0",
    "consolidate": "^0.15.1",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "extend": "^3.0.2",
    "jade": "^1.11.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "pug": "^3.0.0",
    "redis": "^3.0.2",
    "socket.io": "^2.3.0",
    "swig": "^1.4.2"
  }
}
